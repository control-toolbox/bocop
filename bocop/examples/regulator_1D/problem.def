# Definition file

# Dimensions
dim.state 2
dim.control 1
dim.boundaryconditions 3
dim.pathconstraints 0
dim.parameters 0
dim.constants 0

# Time interval
initial.time 0
final.time 2

# Constants

# Time discretisation
ode.discretization midpoint_implicit
time.steps 100

# Bounds for constraints
boundarycond.0.lowerbound 1
boundarycond.0.upperbound 1
boundarycond.1.lowerbound 0
boundarycond.1.upperbound 0
boundarycond.2.lowerbound 0.5
boundarycond.2.upperbound 0.5

# Bounds for variables
control.0.lowerbound -1
control.0.upperbound 1

# Initialization for discretized problem
state.0.init 0.1
state.1.init 0.1
control.0.init 0.1

# Names

# Ipopt
ipoptIntOption.print_level 5
ipoptIntOption.max_iter 1000
ipoptStrOption.mu_strategy adaptive
ipoptNumOption.tol 1e-12

# Misc
ad.retape 0
